                  console.log("We just reached " + s + " seconds!");
                  break;
          }*/
        }
        // Sync with Aweber through Ajax
        function tag_aweber(qstring){
          $.ajax({
              type: "POST",
              url: "/php/aweber-api/wistia/tag_sub_with_watch_history.php",
              data: qstring,
              async: true,
