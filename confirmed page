  <script>
    function getQueryVariable(variable)
        {
               var query = window.location.search.substring(1);
               var vars = query.split("&");
               for (var i=0;i<vars.length;i++) {
                       var pair = vars[i].split("=");
                       if(pair[0] == variable){return pair[1];}
               }
               return(false);
        }
    window._wq = window._wq || [];

    // target our video by the first 3 characters of the hashed ID
    _wq.push({ id: "nnu60f36p8", onReady: function(video) {

      // on play, seek the video to X seconds, then unbind so it
      // only happens once.
      video.bind('play', function() {
    
    if(getQueryVariable("svideo")){
      video.time(getQueryVariable('svideo'));
    }
    return video.unbind;
  });
      video.bind('secondchange', function(s) 
      {
        //var svideo = getQueryVariable('svideo');
        if(email !== null && (!getQueryVariable('svideo')))
        {
          var email = video.email();
          var nonce = 1;
          var listname = 'eb-NEXT17plc';
          var tagname = 'plc1-'+s+'s';
          var qstring = 'nonce='+ nonce + '&listname='+ listname + '&email='+ email + '&tagname='+ tagname;
          /*switch(s)
          {
              case 45:
                  tag_aweber(qstring);
